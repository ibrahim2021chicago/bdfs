{
  "regions": {
    "uk": {
      "security": {
        "ukPersonOnly": {
          "allow": [
            "CTSRevRecGSCApproved",
            "CTSRevRecGSCNoFornApproved"
          ],
          "deny": [
            "CTSRevRecUnrestricted"
          ]
        },
        "ukCitizenOnly": {
          "allow": [
            "CTSRevRecGSCNoFornApproved"
          ],
          "deny": [
            "CTSRevRecGSCApproved",
            "CTSRevRecUnrestricted"
          ]
        },
        "unrestricted": {
          "allow": [
            "CTSRevRecGSCApproved",
            "CTSRevRecGSCNoFornApproved",
            "CTSRevRecUnrestricted"
          ],
          "deny": []
        }
      },
      "groups": {
        "CTSRevRecGSCApproved": {},
        "CTSRevRecGSCNoFornApproved": {},
        "CTSRevRecUnrestricted": {}
      },
      "fields": {
        "Marked GSC": {
          "var": "isGSC",
          "type": "bool"
        },
        "Marked UK Persons Only": {
          "var": "ukPersonOnly",
          "type": "bool",
          "report": true
        },
        "Marked UK Citizen Only": {
          "var": "ukCitizenOnly",
          "type": "bool",
          "report": true
        }
      }
    },
    "us": {
      "security": {
        "usPersonOnly": {
          "allow": [
            "CTSRevRecCUIApproved",
            "CTSRevRecCUINoFornApproved"
          ],
          "deny": [
            "CTSRevRecUnrestricted"
          ]
        },
        "usCitizenOnly": {
          "allow": [
            "CTSRevRecCUINoFornApproved"
          ],
          "deny": [
            "CTSRevRecUnrestricted",
            "CTSRevRecCUIApproved"
          ]
        },
        "unrestricted": {
          "allow": [
            "CTSRevRecCUIApproved",
            "CTSRevRecCUINoFornApproved",
            "CTSRevRecUnrestricted"
          ],
          "deny": []
        }
      },
      "groups": {
        "CTSRevRecCUIApproved": {},
        "CTSRevRecCUINoFornApproved": {},
        "CTSRevRecUnrestricted": {}
      },
      "fields": {
        "Marked CUI-Specific": {
          "var": "isCUI",
          "type": "bool"
        },
        "Marked US Persons Only": {
          "var": "usPersonOnly",
          "type": "bool",
          "report": true
        },
        "Marked US Citizen Only": {
          "var": "usCitizenOnly",
          "type": "bool",
          "report": true
        },
        "Marked ITAR/EAR": {
          "var": "isITAR",
          "type": "bool"
        }
      },
      "validation_rules": [
        {
          "if": "isITAR",
          "requires": ["usCitizenOnly"],
          "forbids": ["usPersonOnly"],
          "message": "ITAR requires US Citizen Only and cannot be US Person Only"
        },
        {
          "if": "isCUI",
          "requires": ["usCitizenOnly"],
          "forbids": ["usPersonOnly"],
          "message": "CUI-Specific requires US Citizen Only and cannot be US Person Only"
        },
        {
          "forbids_both": ["usPersonOnly", "usCitizenOnly"],
          "message": "Cannot be both US Person Only and US Citizen Only"
        },
        {
          "if": "isReady",
          "requires": ["isCompleted"],
          "message": "Ready requires Completed"
        }
      ]
    },
    "in": {}
  },
  "sources": [
